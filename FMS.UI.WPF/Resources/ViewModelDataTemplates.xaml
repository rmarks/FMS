<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:FMS.WPF.UI.Resources"
                    xmlns:vm="clr-namespace:FMS.WPF.ViewModels;assembly=FMS.WPF.ViewModel"
                    xmlns:vw="clr-namespace:FMS.WPF.Views">

    <DataTemplate DataType="{x:Type vm:CompaniesViewModel}">
        <vw:CompaniesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CompaniesListViewModel}">
        <vw:CompaniesListView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CompanyCompoundViewModel}">
        <vw:CompanyCompoundView/>
    </DataTemplate>

</ResourceDictionary>